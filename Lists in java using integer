import java.util.List;
import java.util.ArrayList;

class Main {
    public static void main(String[] args) {
        List<Integer> list = new ArrayList<>(); //syntax to create list in java
        list.add(1);
        list.add(2);
        list.add(3);
        list.add(4);
        list.add(5);
        System.out.println("numbers");
        System.out.println("-----");
        System.out.println(list);
    }
}
